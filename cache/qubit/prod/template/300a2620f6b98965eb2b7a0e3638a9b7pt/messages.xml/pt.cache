eitáveis: AAAAMMDD, AAAA-MM-DD, AAAA-MM, AAAA.";i:1;s:3:"950";i:2;s:0:"";}s:7:"Name(s)";a:3:{i:0;s:7:"Nome(s)";i:1;s:3:"951";i:2;s:0:"";}s:10:"Actor name";a:3:{i:0;s:26:"Nome da entidade produtora";i:1;s:3:"952";i:2;s:0:"";}s:71:"Identify and record the name(s) and type(s) of the unit of description.";a:3:{i:0;s:72:"Identificar e registar o(s) nome(s) e tipo(s) da unidade de descrição.";i:1;s:3:"953";i:2;s:0:"";}s:7:"Add new";a:3:{i:0;s:14:"Adicionar novo";i:1;s:3:"954";i:2;s:0:"";}}